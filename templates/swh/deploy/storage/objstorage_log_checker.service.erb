# File managed by puppet - modifications will be overwritten
[Unit]
Description=ObjStorage Content Log Checker
After=network.target

[Service]
ExecStart=python3 -m swh.objstorage.checker log --daemon
ExecReload=/bin/kill -HUP $MAINPID
KillMode=process
Restart=on-failure

[Install]
WantedBy=multi-user.target
