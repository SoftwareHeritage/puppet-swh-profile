storage:
  cls: <%= @checker_class %>
  args:
    root: <%= @directory %>
    slicing: <%= @slicing %>

batch_size: <%= @batch_size %>
log_tag: <%= @log_tag %>
storage_name: <%= @storage_name %>
dbconn: host=<% @db_host %> dbname=<%= @db_dbname %> user=<%= @db_user %> password=<%= @db_password %>
