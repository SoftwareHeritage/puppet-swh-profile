# UWSGI configuration for swh.storage.api
# Managed by puppet - changes will be overwritten
[uwsgi]
plugin = python3

protocol = <%= @uwsgi_protocol %>
socket = <%= @uwsgi_listen_address %>

workers = <%= @uwsgi_workers %>

max-requests = <%= @uwsgi_max_requests %>
max-requests-delta = <%= @uwsgi_max_requests_delta %>
worker-reload-mercy = <%= @uwsgi_reload_mercy %>
reload-mercy = <%= @uwsgi_reload_mercy %>

uid = <%= @user %>
gid = <%= @user %>

module = swh.storage.api.server
callable = run_from_webserver
